{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "buildCommand": "ng build --configuration production && echo \"window._env_ = { API_KEY: '$API_KEY' };\" > dist/fashion-virtual-try-on/assets/env.js",
        "outputDirectory": "dist/fashion-virtual-try-on"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 